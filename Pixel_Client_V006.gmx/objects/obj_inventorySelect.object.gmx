            <string>/// select item

scr_detectKey();

if (!rightKey &amp;&amp; !leftKey &amp;&amp; !downKey &amp;&amp; !upKey)
    {
        run = 0;
    }
    
if (run == 0) //if you let go of all commands
    {
        if(rightKey)
            {
                global.inventorySlot += 1;
                if(global.inventorySlot &gt; 25)
                    global.inventorySlot = 1;
                run = 1;
            }
        if(leftKey)
            {
                global.inventorySlot -= 1;
                if(global.inventorySlot &lt; 1)
                    global.inventorySlot = 25;
                run = 1;
            }
        if(downKey)
            {
                global.inventorySlot += 5;
                if(global.inventorySlot &gt; 25)
                    global.inventorySlot -= 25;
                run = 1;
            }
        if(upKey)
            {
                global.inventorySlot -= 5;
                if(global.inventorySlot &lt; 1)
                    global.inventorySlot += 25;
                run = 1;
            }
    }
</string>
